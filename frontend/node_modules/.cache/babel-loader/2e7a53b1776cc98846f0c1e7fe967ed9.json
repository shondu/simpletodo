tial\" v-if=\"items.length != 0\">\n        <ul class=\"mainlist\">\n            <li v-for=\"item in items\" :key=\"item.id\">\n                {{item.value}}\n                <span @click=\"removeEntry(item.id)\">x</span>\n            </li>\n        </ul>\n        <hr class=\"hrpartial\" v-if=\"items.length != 0\">\n        <div>\n            <label for=\"newitem\">What would you like to accomplish: </label>\n            <input id=\"newitem\" type=\"text\" v-model=\"newItemValue\" @keypress.enter=\"addItem()\">\n        </div>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: \"todo_page\",\n        data: function () {\n            return {\n                items: [],\n                newItemValue: \"\",\n                currentId: 0,\n                selectedItem: 0,\n            }\n        },\n        methods: {\n            addItem: function() {\n                this.items.push({\n                    id: this.currentId,\n                    value: this.newItemValue\n                });\n                this.newItemValue = '';\n                this.currentId++;\n            },\n            removeEntry: function (itemIndex) {\n                let foundIndex = -1;\n                for(let index = 0; index < this.items.length; index++) {\n                    if (this.items[index].id === itemIndex) {\n                        foundIndex = index;\n                        break;\n                    }\n                }\n\n                if (foundIndex > -1) {\n                    this.items.splice(foundIndex, 1);\n                }\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .mainlist{\n        display: block;\n        margin-left: auto;\n        margin-right: auto;\n        width: 50%;\n        text-align: left;\n        list-style-type: none;\n    }\n    .hrpartial {\n        display: block;\n        margin-left: auto;\n        margin-right: auto;\n        width: 60%;\n    }\n    li > span {\n        float: right;\n        cursor: pointer;\n    }\n</style>"],"sourceRoot":"src/components"}]}