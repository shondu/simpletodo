KAAA;AASA,GA5BA;AA6BA,EAAA,OAAA,EAAA;AACA,IAAA,OAAA,EAAA,mBAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA;AACA,QAAA,EAAA,EAAA,KAAA,SADA;AAEA,QAAA,SAAA,EAAA,KAAA,YAFA;AAGA,QAAA,SAAA,EAAA;AAHA,OAAA;AAKA,WAAA,YAAA,GAAA,EAAA;AACA,WAAA,SAAA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,YAAA,EAAA;AACA,QAAA,OAAA,EAAA;AADA,OAAA;AAGA,KAbA;AAcA,IAAA,WAAA,EAAA,qBAAA,SAAA,EAAA;AACA,UAAA,UAAA,GAAA,CAAA,CAAA;;AACA,WAAA,IAAA,KAAA,GAAA,CAAA,EAAA,KAAA,GAAA,KAAA,KAAA,CAAA,MAAA,EAAA,KAAA,EAAA,EAAA;AACA,YAAA,KAAA,KAAA,CAAA,KAAA,EAAA,EAAA,KAAA,SAAA,EAAA;AACA,UAAA,UAAA,GAAA,KAAA;AACA;AACA;AACA;;AAEA,UAAA,UAAA,GAAA,CAAA,CAAA,EAAA;AACA,aAAA,KAAA,CAAA,MAAA,CAAA,UAAA,EAAA,CAAA;AACA,aAAA,KAAA,CAAA,YAAA,EAAA;AACA,UAAA,OAAA,EAAA;AADA,SAAA;AAGA,aAAA,SAAA,GAAA,IAAA;AACA;AACA,KA9BA;AA+BA,IAAA,QAAA,EAAA,oBAAA;AACA,WAAA,SAAA,GAAA,KAAA;;AACA,UAAA,KAAA,SAAA,EAAA;AACA,YAAA,OAAA,GAAA,kCAAA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,OAAA,EAAA,KAAA,KAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,SAHA,EAIA,KAJA,CAIA,UAAA,KAAA;AAAA,iBAAA,OAAA,CAAA,KAAA,CAAA,KAAA,CAAA;AAAA,SAJA;AAKA,OAPA,MAOA;AACA,YAAA,KAAA,KAAA,CAAA,MAAA,EAAA;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,KAAA,cAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,KAAA,CAAA;AACA;AACA;AACA,KA7CA;AA8CA,IAAA,WAAA,EAAA,qBAAA,MAAA,EAAA;AACA,UAAA,QAAA,GAAA,KAAA,QAAA,CAAA,MAAA,CAAA,aAAA,CAAA,EAAA,CAAA;;AACA,UAAA,QAAA,KAAA,IAAA,EAAA;AACA,QAAA,QAAA,CAAA,SAAA,GAAA,MAAA,CAAA,aAAA,CAAA,OAAA;AACA;;AACA,WAAA,SAAA,GAAA,IAAA;AACA,KApDA;AAqDA,IAAA,QAAA,EAAA,kBAAA,MAAA,EAAA;AACA,UAAA,UAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,EAAA,KAAA,QAAA,CAAA,MAAA,EAAA,EAAA,CAAA,EAAA;AACA,UAAA,UAAA,GAAA,QAAA;AACA;AACA,OAJA;AAMA,aAAA,UAAA;AACA,KA9DA;AA+DA,IAAA,kBAAA,EAAA,8BAAA;AAAA;;AAEA,UAAA,UAAA,GAAA,CAAA;AAEA,WAAA,KAAA,CAAA,OAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,SAAA,EAAA;AACA,UAAA,MAAA,CAAA,YAAA,CAAA,IAAA,CAAA,QAAA,CAAA,EAAA;AACA;;AACA,YAAA,QAAA,CAAA,EAAA,GAAA,MAAA,CAAA,SAAA,EAAA;AACA,UAAA,UAAA,GAAA,QAAA,CAAA,EAAA;AACA;AACA,OAPA;AASA,MAAA,UAAA;AACA,aAAA,UAAA;AAEA;AA/EA;AA7BA,CAAA","sourcesContent":["<template>\n    <div>\n        <h1>Whatcha thinking of doing?</h1>\n        <hr class=\"hrpartial\" v-if=\"items.length !== 0\">\n        <div class=\"title_bar\">\n            <span style=\"float: left; margin-right: 8px\">Done</span>\n            <span style=\"float: left\">Item</span>\n            <span style=\"float: right\">Remove</span>\n        </div>\n        <br>\n        <ul class=\"mainlist\">\n            <li v-for=\"item in items\" :key=\"item.id\" v-bind:class=\"{ item_completed : item.completed}\">\n                <input type=\"checkbox\"\n                       :id=\"item.id\"\n                       :value=\"item.id\"\n                       @click=\"itemClicked\"\n                       v-model=\"selectedItem\"\n                        class=\"done_check\">\n                {{item.todoValue}}\n                <span @click=\"removeEntry(item.id)\">x</span>\n            </li>\n        </ul>\n        <hr class=\"hrpartial\" v-if=\"items.length !== 0\">\n        <div class=\"input_div\">\n            <label for=\"newitem\">What would you like to accomplish: </label>\n            <input id=\"newitem\" type=\"text\" v-model=\"newItemValue\" @keypress.enter=\"addItem()\">\n            <button v-if=\"items.length !== 0 && canUpdate\" class=\"save_button\" @click=\"saveData()\">Save List</button>\n        </div>\n    </div>\n</template>\n\n<script>\n    import axios from 'axios'\n    export default {\n        name: \"todo_page\",\n        created() {\n            if(this.useServer) {\n                const todoUrl = 'http://localhost:9000/data/todos';\n                axios.get(todoUrl).then(response => {\n                    console.log(response)\n                    this.items = response.data;\n                    this.currentId = this.calculateCurrentId();\n                }).catch(error => console.error(error));\n            }else{\n                const retrievedData = localStorage.getItem(this.todoItemString);\n                if(retrievedData) {\n                    this.items = JSON.parse(retrievedData);\n                    this.currentId = this.calculateCurrentId();\n                }\n            }\n        },\n        data: function () {\n            return {\n                items: [],\n                newItemValue: '',\n                todoItemString: 'todoItems',\n                currentId: 0,\n                selectedItem: [],\n                useServer: true,\n                canUpdate: false\n            }\n        },\n        methods: {\n            addItem: function() {\n                this.items.push({\n                    id: this.currentId,\n                    todoValue: this.newItemValue,\n                    completed: false\n                });\n                this.newItemValue = '';\n                this.currentId++;\n                this.canUpdate = true;\n                this.$emit('todo-event', {\n                    message: 'Added An Item'\n                })\n            },\n            removeEntry: function (itemIndex) {\n                let foundIndex = -1;\n                for(let index = 0; index < this.items.length; index++) {\n                    if (this.items[index].id === itemIndex) {\n                        foundIndex = index;\n                        break;\n                    }\n                }\n\n                if (foundIndex > -1) {\n                    this.items.splice(foundIndex, 1);\n                    this.$emit('todo-event', {\n                        message: 'Removed An Item'\n                    })\n                    this.canUpdate = true;\n                }\n            },\n            saveData: function () {\n                this.canUpdate = false;\n                if(this.useServer) {\n                    const todoUrl = 'http://localhost:9000/data/todos';\n                    axios.put(todoUrl, this.items)\n                        .then(response => {\n                            console.log(response)\n                        })\n                        .catch(error => console.error(error));\n                } else {\n                    if (this.items.length) {\n                        localStorage.setItem(this.todoItemString, JSON.stringify(this.items));\n                    }\n                }\n            },\n            itemClicked: function($event) {\n                let workItem = this.findItem($event.currentTarget.id);\n                if (workItem !== null) {\n                    workItem.completed = $event.currentTarget.checked;\n                }\n                this.canUpdate = true;\n            },\n            findItem: function(itemId){\n                let returnItem = null;\n                this.items.forEach(todoItem => {\n                    if(todoItem.id === parseInt(itemId, 10)){\n                        returnItem = todoItem;\n                    }\n                })\n\n                return returnItem;\n            },\n            calculateCurrentId: function(){\n\n                let returnedId = 0;\n\n                this.items.forEach(todoItem => {\n                    if(todoItem.completed){\n                        this.selectedItem.push(todoItem.id);\n                    }\n                    if(todoItem.id > this.currentId) {\n                        returnedId = todoItem.id;\n                    }\n                });\n\n                returnedId++;\n                return returnedId;\n\n            },\n        }\n    }\n</script>\n\n<style scoped>\n    .mainlist{\n        display: block;\n        margin-left: auto;\n        margin-right: auto;\n        width: 50%;\n        text-align: left;\n        list-style-type: none;\n    }\n    .hrpartial {\n        display: block;\n        margin-left: auto;\n        margin-right: auto;\n        width: 60%;\n    }\n    .input_div{\n        display: block;\n        margin-left: auto;\n        margin-right: auto;\n        width: 60%;\n    }\n    .save_button{\n        float: right;\n    }\n    li > span {\n        float: right;\n        cursor: pointer;\n        margin-right: 50px;\n    }\n    .title_bar{\n        display: block;\n        margin-left: auto;\n        margin-right: auto;\n        width: 50%;\n    }\n    .item_completed {\n        text-decoration: line-through;\n    }\n    .done_check {\n        margin-left: -10px;\n        margin-right: 15px;\n    }\n</style>"],"sourceRoot":"src/components"}]}